
-- Metadata
meta.id = "Test.Control.Toggle"
meta.name = "Test Control Toggle"
meta.author = "Test"

events.action = function (name, extras)
	print("action: " .. name);
end

actions.change = function (checked)
	local b = "";
	if (checked) then 
		b = "true";
	else 
		b = "false";
	end
	print("action: change: " .. b);
end