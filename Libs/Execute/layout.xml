<?xml version="1.0" encoding="utf-8"?>
<layout>
	<row>
		<text id="output" />
	</row>
	<row weight="wrap">
		<text id="command" onchange="change" />
		<button id="button" weight="wrap" text="Run" ontap="run" />
	</row>
	<row>
		<button text="exec" ontap="execute" />
		<button text="open" ontap="open" />
		<button text="start" ontap="start" />
	</row>
</layout>